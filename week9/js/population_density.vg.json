{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "2023 Australian State Population Density",
    "width": 800,
    "height": 400,
    "projection": {"type": "equalEarth"},
    "data": {
        "url": "https://raw.githubusercontent.com/twuu0064/3179/main/week9/js/ausStates.json",
        "format": {"type": "topojson", "feature": "STE_2021_AUST_GDA2020"}
    },
    "transform": [
        {
        "lookup": "properties.STE_NAME21",
        "from": {
        "data": {
        "url": "https://raw.githubusercontent.com/twuu0064/3179/main/week9/data/PopulationDensity.csv"
        },
        "key": "State/Territory",
        "fields": ["Population Density"]
        }
        }
    ],
    "mark": {"type": "geoshape"},
    "encoding": {
        "color": {
        "field": "Population Density",
        "type": "quantitative",
        "title": "Population Density (Persons/km2)",
        "scale": {"type": "log", "scheme": "greenblue"}
        },

        "tooltip": [
            {"field": "properties.STE_NAME21", "type": "nominal", "title":
            "State"},
            {"field": "Population Density", "type": "quantitative"}
            ]
    }
    }